{
  "name": "svc/contactform-bundle",
  "description": "Display a contactform, send a mail",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^7.3.0 | ^8.0.0",
    "symfony/framework-bundle": "^5",
    "symfony/options-resolver": "^5",
    "svc/util-bundle": "^1|dev-main",
    "symfony/translation": "^5",
    "twig/twig": "^2.12|^3.0",
    "symfony/twig-bundle": "^4.3|^5.0",
    "symfony/form": "^5"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ContactformBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ContactformBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5",
    "symfony/browser-kit": "^5"
  }
}
